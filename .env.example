# Apatte Pitbox - Environment Template
# Copy to .env for local development.
# Do NOT commit real secrets.

# General
NODE_ENV=development
LOG_LEVEL=info

# Supabase (future use)
# Required for web-dashboard once implemented
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Optional for now. DO NOT expose to client.
SUPABASE_SERVICE_ROLE_KEY=

# MQTT (future use)
# Required for telemetry-ingestion and edge-agent once implemented
MQTT_BROKER_URL=mqtt://localhost:1883
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_CLIENT_ID=

# Telemetry ingestion defaults
TELEMETRY_TOPIC_PREFIX=apatte
INGESTION_BATCH_SIZE=200
INGESTION_FLUSH_MS=250

# App ports (optional)
WEB_PORT=
INGESTION_PORT=
EDGE_PORT=
